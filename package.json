  "jest": {
    "testEnvironment": "jest-environment-jsdom",
    "modulePaths": [
      "/src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "yaml",
      "yml",
      "json"
    ],
    "transform": {
      "^.+\\.[jt]sx?$": "babel-jest",
      "^.+\\.ya?ml$": "yaml-jest-transform",
      "^.+\\.txt$": "jest-raw-loader"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!@cfworker|webext-detect-page|idb|webext-patterns|webext-polyfill-kinda|webext-additional-permissions|p-timeout|p-defer)"
    ],
    "setupFiles": [
      "<rootDir>/src/test-env.js",
      "jest-webextension-mock",
      "fake-indexeddb/auto"
    ],
    "moduleNameMapper": {
      "^@/icons/svgIcons$": "<rootDir>/src/__mocks__/iconsMock",
      "^@/(.*)$": "<rootDir>/src/$1",
      "^vendors/(.*)$": "<rootDir>/src/vendors/$1",
      "^@contrib/(.*)$": "<rootDir>/contrib/$1",
      "^@schemas/(.*)$": "<rootDir>/schemas/$1",
      "^@/chrome$": "<rootDir>/src/__mocks__/chromeMock.ts",
      "\\.(css|less)$": "<rootDir>/src/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/src/__mocks__/fileMock.js"
    }
  },
  "webExt": {
    "sourceDir": "browsers/dist"
  }
