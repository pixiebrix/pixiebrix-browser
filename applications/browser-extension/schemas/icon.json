{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://app.pixiebrix.com/schemas/icon#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "library": {
      "type": "string",
      "enum": ["bootstrap", "simple-icons", "custom"]
    },
    "color": {
      "type": "string",
      "default": "#ae87e8"
    },
    "size": {
      "type": "number",
      "default": 14,
      "minimum": 1
    }
  },
  "required": ["id", "library"]
}
